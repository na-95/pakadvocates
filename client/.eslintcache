\r\n                        <form onSubmit={this.submitForm}>\r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-user\"></i> </span>\r\n                                </div>\r\n                                <input onChange={this.handleForm} required name=\"firstName\" className=\"form-control\" placeholder=\"First name\" type=\"text\"/>\r\n                            </div>\r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-user\"></i> </span>\r\n                                </div>\r\n                                <input onChange={this.handleForm} required name=\"lastName\" className=\"form-control\" placeholder=\"Last name\" type=\"text\"/>\r\n                            </div> \r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-envelope\"></i> </span>\r\n                                </div>\r\n                                <input onChange={this.handleForm} required name=\"email\" className=\"form-control\" placeholder=\"Email address\" type=\"email\"/>\r\n                            </div> \r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-phone\"></i> </span>\r\n                                </div>\r\n                                <select className=\"custom-select\" style={{maxWidth: '120px'}}>\r\n                                    <option selected=\"\">+92</option>\r\n                                    {/* <option value=\"1\">+972</option>\r\n                                    <option value=\"2\">+198</option>\r\n                                    <option value=\"3\">+701</option> */}\r\n                                </select>\r\n                                <input onChange={this.handleForm} required name=\"phoneNumber\" className=\"form-control\" placeholder=\"Phone number\" type=\"text\"/>\r\n                            </div>\r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-building\"></i> </span>\r\n                                </div>\r\n                                <select onChange={this.handleForm} name=\"userType\" required className=\"form-control\">\r\n                                    <option value=\"\" selected> Who are you?</option>\r\n                                    <option value=\"client\">Client</option>\r\n                                    <option value=\"lawyer\">Lawyer</option>\r\n                                </select>\r\n                            </div>\r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-lock\"></i> </span>\r\n                                </div>\r\n                                <input onChange={this.handleForm} name=\"password\" required className=\"form-control\" placeholder=\"Create password\" type=\"password\"/>\r\n                            </div> \r\n                            <div className=\"form-group input-group\">\r\n                                <div className=\"input-group-prepend\">\r\n                                    <span className=\"input-group-text\"> <i className=\"fa fa-lock\"></i> </span>\r\n                                </div>\r\n                                <input onChange={this.handleForm} name=\"repeatPassword\" required className=\"form-control\" placeholder=\"Repeat password\" type=\"password\"/>\r\n                            </div>                                      \r\n                            <div className=\"form-group\">\r\n                                <button type=\"submit\" className=\"btn btn-primary btn-block\"> Create Account  </button>\r\n                            </div>      \r\n                            <p className=\"text-center\">Have an account? <a href=\"\">Log In</a> </p>                                                                 \r\n                        </form>\r\n                    </article>\r\n                </div>\r\n            </Container> \r\n        )\r\n    }\r\n}\r\n\r\nexport default connect(null, { postLawyer })(SignUp)","C:\\Users\\Nouraiz\\Desktop\\Workspace\\pakadvocates\\client\\src\\page-components\\AddCourtCategory.js",[],"C:\\Users\\Nouraiz\\Desktop\\Workspace\\pakadvocates\\client\\src\\page-components\\ThankYou.js",[],"C:\\Users\\Nouraiz\\Desktop\\Workspace\\pakadvocates\\client\\src\\page-components\\LawyerRequests.js",["217","218"],"import React, { Component } from 'react';\r\nimport { Button, ButtonGroup, Container } from 'react-bootstrap';\r\nimport { connect } from \"react-redux\";\r\nimport { getUnapprovedLawyers, deleteLawyer, putLawyer } from '../actions';\r\n\r\nlet unapprovedLawyers;\r\nconst mapStateToProps = state => {\r\n    unapprovedLawyers = state.LawyerReducer.unapprovedLawyers;\r\n\r\n    return state;\r\n}\r\n\r\nclass LawyerRequests extends Component {\r\n\r\n    componentDidMount = () => {\r\n        this.props.getUnapprovedLawyers();\r\n    }\r\n\r\n    rejectLawyerRequest = (lawyerId) => async () => {\r\n        // console.log(lawyerId)\r\n        await this.props.deleteLawyer(lawyerId);\r\n        this.props.getUnapprovedLawyers();\r\n    }\r\n\r\n    acceptLawyerRequest = (lawyerId) => async () => {\r\n        await this.props.putLawyer({ approval_status: 1 }, lawyerId);\r\n        this.props.getUnapprovedLawyers();\r\n    }\r\n\r\n    render() {\r\n\r\n        console.log('LawyersRequests rendered')\r\n        return (\r\n            <Container className=\"py-4 \">\r\n                <div className=\"card bg-light\">\r\n                    <article className=\"card-body mx-auto\" style={{ maxWidth: \"600px\" }}>\r\n                        <h4 className=\"card-title mt-3 text-center\">Approve/Reject Pending Lawyer Requests</h4>\r\n                        <hr />\r\n                        <ul className=\"requests-list p-0\" >\r\n                            {\r\n                                unapprovedLawyers.length > 0 &&\r\n                                unapprovedLawyers.map(item => (\r\n                                    <li key={item.id.toString()} className='unstyled my-4 list-group'>\r\n                                        <span className=\"list-group-item\">{`${item.first_name} ${item.last_name}`}</span>\r\n                                        <span className=\"list-group-item\">{`${item.email}`}</span>\r\n                                        <span role=\"button\" className=\"text-center font-weight-bold text-dark list-group-item list-group-item-success\" onClick={this.acceptLawyerRequest(item.id.toString())}>Approve</span>\r\n                                        <span role=\"button\" className=\"text-center font-weight-bold text-dark list-group-item list-group-item-danger\" onClick={this.rejectLawyerRequest(item.id.toString())}>Reject</span>\r\n                                    </li>\r\n                                ))\r\n                            }\r\n                            {\r\n                                unapprovedLawyers.length === 0 &&\r\n 